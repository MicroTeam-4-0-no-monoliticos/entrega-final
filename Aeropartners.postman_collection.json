						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Guardar id_pago (colección)\", function () {",
									"  const { id_pago } = pm.response.json();",
									"  pm.collectionVariables.set(\"id_pago\", id_pago);",
									"});"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
